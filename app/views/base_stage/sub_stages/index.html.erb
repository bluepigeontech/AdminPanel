<p id="notice"><%= notice %></p>

<h1>Listing Stage Sub Stages</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stage_sub_stages.each do |stage_sub_stage| %>
      <tr>
        <td><%= link_to 'Show', stage_sub_stage %></td>
        <td><%= link_to 'Edit', edit_stage_sub_stage_path(stage_sub_stage) %></td>
        <td><%= link_to 'Destroy', stage_sub_stage, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

