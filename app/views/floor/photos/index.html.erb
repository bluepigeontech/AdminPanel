<p id="notice"><%= notice %></p>

<h1>Listing Floor Photos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @floor_photos.each do |floor_photo| %>
      <tr>
        <td><%= link_to 'Show', floor_photo %></td>
        <td><%= link_to 'Edit', edit_floor_photo_path(floor_photo) %></td>
        <td><%= link_to 'Destroy', floor_photo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Photo', new_floor_photo_path %>
