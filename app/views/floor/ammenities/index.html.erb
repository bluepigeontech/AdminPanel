<p id="notice"><%= notice %></p>

<h1>Listing Floor Ammenities</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @floor_ammenities.each do |floor_ammenity| %>
      <tr>
        <td><%= link_to 'Show', floor_ammenity %></td>
        <td><%= link_to 'Edit', edit_floor_ammenity_path(floor_ammenity) %></td>
        <td><%= link_to 'Destroy', floor_ammenity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ammenity', new_floor_ammenity_path %>
