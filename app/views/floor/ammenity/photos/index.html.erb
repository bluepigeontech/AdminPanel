<p id="notice"><%= notice %></p>

<h1>Listing Floor Ammenity Photos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @floor_ammenity_photos.each do |floor_ammenity_photo| %>
      <tr>
        <td><%= link_to 'Show', floor_ammenity_photo %></td>
        <td><%= link_to 'Edit', edit_floor_ammenity_photo_path(floor_ammenity_photo) %></td>
        <td><%= link_to 'Destroy', floor_ammenity_photo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Photo', new_floor_ammenity_photo_path %>
