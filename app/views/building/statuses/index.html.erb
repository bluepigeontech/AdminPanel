<p id="notice"><%= notice %></p>

<h1>Listing Building Statuses</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @building_statuses.each do |building_status| %>
      <tr>
        <td><%= link_to 'Show', building_status %></td>
        <td><%= link_to 'Edit', edit_building_status_path(building_status) %></td>
        <td><%= link_to 'Destroy', building_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Status', new_building_status_path %>
