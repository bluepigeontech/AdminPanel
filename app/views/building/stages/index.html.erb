<p id="notice"><%= notice %></p>

<h1>Listing Building Stages</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @building_stages.each do |building_stage| %>
      <tr>
        <td><%= link_to 'Show', building_stage %></td>
        <td><%= link_to 'Edit', edit_building_stage_path(building_stage) %></td>
        <td><%= link_to 'Destroy', building_stage, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stage', new_building_stage_path %>
