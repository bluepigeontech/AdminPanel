<p id="notice"><%= notice %></p>

<h1>Listing Flat Photos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @flat_photos.each do |flat_photo| %>
      <tr>
        <td><%= link_to 'Show', flat_photo %></td>
        <td><%= link_to 'Edit', edit_flat_photo_path(flat_photo) %></td>
        <td><%= link_to 'Destroy', flat_photo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Photo', new_flat_photo_path %>
