<p id="notice"><%= notice %></p>

<h1>Listing Project Offers</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @project_offers.each do |project_offer| %>
      <tr>
        <td><%= link_to 'Show', project_offer %></td>
        <td><%= link_to 'Edit', edit_project_offer_path(project_offer) %></td>
        <td><%= link_to 'Destroy', project_offer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Offer', new_project_offer_path %>
