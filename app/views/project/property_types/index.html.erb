<p id="notice"><%= notice %></p>

<h1>Listing Project Property Types</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @project_property_types.each do |project_property_type| %>
      <tr>
        <td><%= link_to 'Show', project_property_type %></td>
        <td><%= link_to 'Edit', edit_project_property_type_path(project_property_type) %></td>
        <td><%= link_to 'Destroy', project_property_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Property type', new_project_property_type_path %>
