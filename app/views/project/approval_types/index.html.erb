<p id="notice"><%= notice %></p>

<h1>Listing Project Approval Types</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @project_approval_types.each do |project_approval_type| %>
      <tr>
        <td><%= link_to 'Show', project_approval_type %></td>
        <td><%= link_to 'Edit', edit_project_approval_type_path(project_approval_type) %></td>
        <td><%= link_to 'Destroy', project_approval_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Approval type', new_project_approval_type_path %>
