<h1>Listing Project Home Loan Banks</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @project_home_loan_banks.each do |project_home_loan_bank| %>
      <tr>
        <td><%= link_to 'Show', project_home_loan_bank %></td>
        <td><%= link_to 'Edit', edit_project_home_loan_bank_path(project_home_loan_bank) %></td>
        <td><%= link_to 'Destroy', project_home_loan_bank, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

