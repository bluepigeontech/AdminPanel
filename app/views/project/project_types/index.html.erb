<p id="notice"><%= notice %></p>

<h1>Listing Project Project Types</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @project_project_types.each do |project_project_type| %>
      <tr>
        <td><%= link_to 'Show', project_project_type %></td>
        <td><%= link_to 'Edit', edit_project_project_type_path(project_project_type) %></td>
        <td><%= link_to 'Destroy', project_project_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

